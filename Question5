#include <iostream>
#include <string>

using namespace std;

class Student
{
private:
    int scores[5];

public:
   
    void input()
    {
        cout << "Enter scores for 5 subjects:" << endl;
        for (int i = 0; i < 5; i++)
        {
            cout << "Score " << (i + 1) << ": ";
            cin >> scores[i];
        }
    }

    
    int calculateTotalScore()
    {
        int sum = 0;  
        for (int i = 0; i < 5; i++)
        {
            sum += scores[i];
        }
        return sum;
    }
};

int main()
{
    
    Student students[5];
    int annaTotalScore;
    
    
    for (int i = 0; i < 5; i++)
    {
        cout << "Enter details for student " << (i + 1) << ":" << endl;
        students[i].input();
    }

    
    cout << "Enter scores for Anna:" << endl;
    students[0].input();
    annaTotalScore = students[0].calculateTotalScore();

    
    int countHigher = 0;
    for (int i = 1; i < 5; i++)
    {
        int totalScore = students[i].calculateTotalScore();
        if (totalScore > annaTotalScore)
        {
            countHigher++;
        }
    }

    cout << "Number of students who scored higher than Anna: " << countHigher << endl;

    return 0;
}
